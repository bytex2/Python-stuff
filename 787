import tkinter
from tkinter import Tk
import os
import time
from tkinter import *
from logging import root
import pyautogui


root = Tk()

def Planes():
    os.startfile("C:\Program Files (x86)\Microsoft\Edge\Application\msedge.exe")
    time.sleep (2)
    pyautogui.keyDown("7")
    pyautogui.keyDown("8")
    pyautogui.keyDown("7")
    pyautogui.keyDown("enter")
    time.sleep (2)
    pyautogui.keyDown("tab")
    pyautogui.keyDown("tab")
    pyautogui.keyDown("enter")



Button(root,text="Planes", command=Planes).pack()


root.mainloop()
